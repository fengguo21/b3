/* pages/map/map.wxss */
.imgwrap{
  width: 322rpx;
  height: 144rpx;
  margin-top: 390rpx;
  margin-left: 50%;
  transform: translateX(-50%);

}

.imgwrap image{
  width: 322rpx;
  height: 144rpx;
}
.tip{
  font-size: 40rpx;
  color: rgb(102,102,102);
  text-align: center;
  margin-top: 25rpx;
}

.lines,.times{
  height: 76rpx;
  width: 252rpx;
  font-size: 32rpx;
  color: white;
  text-align: center;
  line-height: 76rpx;
  border-radius: 7rpx;

}
.lines{
  background: #75b2f5;
  flex: 1;
  margin-right: 40rpx;
}
.times{
  background: #f1a56b;
  flex: 1;

}