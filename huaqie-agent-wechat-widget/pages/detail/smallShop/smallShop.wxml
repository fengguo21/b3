<text class="swiper-title">适用行业</text>
</view>
<swiper indicator-dots="{{indicatorDots}}"  id="swiper" circular="{{circular}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">  
      <block wx:for="{{imgUrls}}">  
        <swiper-item>  
           <navigator url="{{item.link}}" hover-class="navigator-hover">  
            <image src="{{item.url}}" class="slide-image" width="355" height="150" mode="widthFix"/>  
            <view class="sname"><text>{{item.title}}</text></view>
           </navigator>   
        </swiper-item>  
        <div class="swiper-button-prev"></div>
    <div class="swiper-button-next"></div>
      </block>  
</swiper>




<image src="../../../images/leftarrow.png" class="left"></image>
<image src="../../../images/rightarrow.png" class="right"></image>



