<!--takeOut.wxml-->
<view class="banner"><image src="https://dn-huaqie.qbox.me/agent-sd1.png" mode="widthFix"></image></view>

<image src="https://dn-huaqie.qbox.me/agent-t1.png" class="image1" mode="widthFix"></image>
<view ><image src="https://dn-huaqie.qbox.me/agent-d09.png" class="image2" mode="widthFix"></image></view>

<view ><image src="https://dn-huaqie.qbox.me/agent-t4.png" class="image3" mode="widthFix"></image></view>

<view ><image src="https://dn-huaqie.qbox.me/agent-t5.png" class="image4" mode="widthFix"></image></view>


<view class="footer">
<view class="swiper-title-box">
  <text class="swiper-title">适用行业</text>
</view>

<swiper indicator-dots="{{indicatorDots}}"  id="swiper" circular="{{circular}}
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" mode="widthFix">  
      <block wx:for="{{imgUrls}}">  
        <swiper-item>  
           <navigator url="{{item.link}}" hover-class="navigator-hover">  
            <image src="{{item.url}}" class="slide-image"  />  
            <view class="sname"><text>{{item.title}}</text></view>
           </navigator>  
           
        </swiper-item>  
        
      </block>  
</swiper>



<image src="../../../images/leftarrow.png" class="left"  bindtap="swiperclick"></image>
<image src="../../../images/rightarrow.png" class="right"></image>
</view>

<view id="footline">
<view class="line1"></view>
<text class="nocontent">没有更多内容了</text>
<view class="line2"></view>
</view>




