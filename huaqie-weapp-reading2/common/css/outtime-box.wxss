/* 过期提醒 */
.outtime-wrap {
  text-align: right;
}
.outtime-wrap .close-btn {
  position: relative;
  width: 56rpx;
  margin-bottom: -16rpx;
  margin-right: 14rpx;
  z-index: 1;
}
.outtime-box-wrap {
  width: 542rpx;
  height: 428rpx;
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0 46rpx;
  text-align: center;
}
.outtime-box-title {
  line-height: 1;
  padding: 60rpx 0;
  font-size: 46rpx;
  color: #333;
}
.outtime-box-wrap .outtime-box-content {
  font-size: 32rpx;
  color: #666;
  line-height: 1.6;
  text-align: left;
  margin-top: -12rpx;
}
.outtime-box-btn {
  width: 234rpx;
  height: 76rpx;
  border-radius: 76rpx;
  background: #e84213;
  color: #fff;
  font-size: 32rpx;
  line-height: 76rpx;
  margin: 56rpx auto 0;
}