/* 返回首页navbar */

.nav-to-index {
  background: rgba(96, 93, 98, 1);
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 520;
  padding: 0 30rpx;
  color: rgba(255, 255, 255, .8);
}

.navbar-left,
.navbar-right {
  display: flex;
  align-items: center;
  height: 100%;
}
.navbar-right view,
.navbar-left view {
  font-size: 26rpx;
  line-height: 1;
}
.navbar-left image {
  display: block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}
.navbar-right view {
  margin-left: 14rpx;
  height: 100%;
  line-height: 80rpx;
}
.navbar-right .fix-line {
  width: 1rpx;
  height: 26rpx;
  background: rgba(255, 255, 255, .8);
}